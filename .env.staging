# Staging Environment Variables
# Use LinkedIn App with staging callback URLs

# LinkedIn OAuth (staging-specific app)
VITE_LINKEDIN_CLIENT_ID=your_staging_client_id
VITE_LINKEDIN_CLIENT_SECRET=your_staging_client_secret

# Mixpanel (staging project)
VITE_MIXPANEL_TOKEN_DEV=staging_mixpanel_token
VITE_MIXPANEL_TOKEN_PROD=staging_mixpanel_token
VITE_ANALYTICS_ENABLED=true

# DataDog (staging)
VITE_DATADOG_RUM_APPLICATION_ID=staging_datadog_app_id
VITE_DATADOG_RUM_CLIENT_TOKEN=staging_datadog_token
VITE_DATADOG_LOGS_CLIENT_TOKEN=staging_datadog_logs_token
VITE_DATADOG_SITE=datadoghq.com
VITE_TELEMETRY_ENABLED=true

# Debug mode
VITE_DEBUG_MODE=true
VITE_STAGING_ENV=true