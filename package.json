{
  "name": "tiago-portfolio-v2",
  "version": "1.0.0",
  "type": "module",
  "description": "Portfolio moderno com TypeScript e Magic UI",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "deploy": "npm run build && gh-pages -d dist",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run --spec 'cypress/e2e/**/*.cy.ts'",
    "test:e2e:headed": "cypress run --headed --spec 'cypress/e2e/**/*.cy.ts'",
    "test:auth": "cypress run --spec 'cypress/e2e/linkedin-auth.cy.ts'",
    "test:auth:open": "cypress open --e2e --spec 'cypress/e2e/linkedin-auth.cy.ts'",
    "test:components": "cypress run --component",
    "test:all": "npm run test:e2e && npm run test:components"
  },
  "dependencies": {
    "@datadog/browser-logs": "^6.12.3",
    "@datadog/browser-rum": "^6.12.3",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.60.1",
    "@opentelemetry/auto-instrumentations-web": "^0.48.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.202.0",
    "@opentelemetry/instrumentation": "^0.202.0",
    "@opentelemetry/instrumentation-document-load": "^0.47.0",
    "@opentelemetry/instrumentation-fetch": "^0.202.0",
    "@opentelemetry/instrumentation-http": "^0.202.0",
    "@opentelemetry/instrumentation-user-interaction": "^0.47.0",
    "@opentelemetry/instrumentation-xml-http-request": "^0.202.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.202.0",
    "@opentelemetry/sdk-trace-web": "^2.0.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react-router-dom": "^5.3.3",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.4",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.294.0",
    "mixpanel-browser": "^2.65.0",
    "node-fetch": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-linkedin-login-oauth2": "^2.0.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.2",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.6"
  },
  "devDependencies": {
    "@testing-library/cypress": "^10.0.3",
    "@types/mixpanel-browser": "^2.60.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "cypress": "^14.5.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "gh-pages": "^6.1.0",
    "postcss": "^8.4.31",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "wrangler": "^4.22.0"
  }
}
